{
  "name": "interesting-chess",
  "version": "1.0.0",
  "description": "A chess web app for playing interesting GM positions with friends",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"node server.js\" \"cd client && npm start\"",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "keywords": ["chess", "multiplayer", "websocket", "game"],
  "author": "",
  "license": "MIT"
}
